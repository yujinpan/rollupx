{
  "name": "rollupx",
  "version": "3.0.1",
  "author": "yujinpan",
  "main": "lib/cjs/build.js",
  "module": "lib/es/build.js",
  "types": "types/build.d.ts",
  "files": [
    "bin",
    "lib",
    "types",
    "babel.config.js",
    "tsconfig.json",
    "README.md"
  ],
  "keywords": [
    "rollup",
    "js build",
    "ts build",
    "declaration build",
    "vue build",
    "scss build",
    "less build",
    "docs build"
  ],
  "bin": {
    "rollupx": "bin/rollupx.js"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "publish:beta": "npm version prerelease --preid=beta && npm publish",
    "publish:patch": "npm version patch && npm publish",
    "publish:minor": "npm version minor && npm publish",
    "version": "npm run build",
    "postversion": "git push --all && git push --tags",
    "build": "node scripts/build.js",
    "test": "jest"
  },
  "dependencies": {
    "@babel/core": "^7.18.10",
    "@babel/plugin-proposal-export-default-from": "^7.18.6",
    "@babel/plugin-proposal-optional-chaining": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@vue/babel-preset-app": "^5.0.8",
    "autoprefixer": "^10.4.7",
    "core-js": "^3",
    "glob": "^8.0.3",
    "gulp": "^4",
    "gulp-typescript": "^6.0.0-alpha.1",
    "jsdoc-to-markdown": "^7.1.1",
    "postcss": "^8.4.14",
    "postcss-copy-assets": "^0.3.1",
    "postcss-url": "^10.1.3",
    "prettier": ">=1",
    "resolve": "^1.20.0",
    "rollup": "^2",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-visualizer": "^5.6.0",
    "rollup-plugin-vue": "^6.0.0",
    "style-inject": "^0",
    "through2": "^4.0.2"
